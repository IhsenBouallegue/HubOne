{
  "name": "hubone",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx}\"",
    "up": "docker-compose -f docker/hubone/docker-compose.development.yml up --build --force-recreate",
    "up:prod": "docker-compose -f docker/hubone/docker-compose.production.yml up --build --force-recreate"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.23.2",
    "@mantine/core": "7.0.0-beta.0",
    "@mantine/form": "7.0.0-beta.0",
    "@mantine/hooks": "7.0.0-beta.0",
    "@mantine/notifications": "7.0.0-beta.0",
    "@prisma/client": "^5.0.0",
    "@tabler/icons-react": "^2.17.0",
    "@tanstack/react-query": "^4.12.0",
    "@tanstack/react-query-devtools": "^4.12.0",
    "axios": "^1.1.3",
    "framer-motion": "^10.0.0",
    "next": "13.4.19",
    "prettier": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scroll": "^1.8.8",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "18.17.6",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "lefthook": "^1.4.9",
    "postcss": "^8.4.28",
    "postcss-preset-mantine": "^1.7.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^5.0.0",
    "semantic-release": "^21.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/git"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "yarn.lock",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
      }
    ]
  },
  "packageManager": "yarn@3.6.2"
}
