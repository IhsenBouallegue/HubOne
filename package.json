{
  "name": "hub-one",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "typecheck": "tsc --noEmit",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "up": "docker-compose up --build --force-recreate",
    "up:prod": "docker-compose -f docker-compose.production.yml up --build --force-recreate"
  },
  "dependencies": {
    "@mantine/core": "^4.1.4",
    "@mantine/hooks": "^4.1.4",
    "@mantine/next": "^4.1.4",
    "framer-motion": "^6.3.2",
    "next": "12.1.5",
    "prettier": "^2.6.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-scroll": "^1.8.7",
    "tabler-icons-react": "^1.46.0"
  },
  "devDependencies": {
    "@types/node": "17.0.25",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-sznm": "^0.2.1",
    "typescript": "4.6.3"
  }
}