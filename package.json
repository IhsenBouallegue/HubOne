{
  "name": "hub-one",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "typecheck": "tsc --noEmit",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "up": "docker-compose -f docker/docker-compose.yml up --build --force-recreate",
    "up:prod": "docker-compose -f docker/docker-compose.production.yml up --build --force-recreate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mantine/core": "^4.1.4",
    "@mantine/hooks": "^4.1.4",
    "@mantine/next": "^4.1.4",
    "@prisma/client": "^3.15.2",
    "@semantic-release/changelog": "^6.0.1",
    "framer-motion": "^6.3.2",
    "next": "12.1.5",
    "prettier": "^2.6.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-scroll": "^1.8.7",
    "tabler-icons-react": "^1.46.0"
  },
  "devDependencies": {
    "@types/node": "17.0.25",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^8.0.0",
    "prisma": "^3.15.2",
    "typescript": "4.6.3"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/git"
  ],
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "yarn.lock",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  }
}