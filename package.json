{
  "name": "hubone",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json,css,sass}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,css,sass}\"",
    "up": "docker-compose -f docker/hubone/docker-compose.development.yml up --build --force-recreate",
    "up:prod": "docker-compose -f docker/hubone/docker-compose.production.yml up --build --force-recreate",
    "generate": "drizzle-kit generate:pg"
  },
  "dependencies": {
    "@clerk/nextjs": "4.23.5",
    "@mantine/core": "7.0.0-beta.3",
    "@mantine/form": "7.0.0-beta.3",
    "@mantine/hooks": "7.0.0-beta.3",
    "@mantine/notifications": "7.0.0-beta.3",
    "@tabler/icons-react": "^2.17.0",
    "@tanstack/react-query": "^4.12.0",
    "@tanstack/react-query-devtools": "^4.12.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.5",
    "framer-motion": "^10.0.0",
    "next": "13.4.19",
    "postgres": "^3.3.5",
    "prettier": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scroll": "^1.8.8",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@next/eslint-plugin-next": "^13.4.19",
    "@types/node": "18.17.6",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-mantine": "^2.0.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "lefthook": "^1.4.9",
    "pg": "^8.11.3",
    "postcss": "^8.4.28",
    "postcss-preset-mantine": "^1.7.0",
    "postcss-simple-vars": "^7.0.1",
    "semantic-release": "^21.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.2"
}
